{
  "name": "phantom",
  "version": "0.0.0",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4201",
    "build-dev": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run phantom:serve-ssr",
    "serve:ssr": "node dist/phantom/server/main.js",
    "ssr": "ng run phantom:server",
    "build:ssr": "ng build && ng run phantom:server",
    "prerender": "ng run phantom:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/cdk": "^16.0.0",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/material": "^16.0.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/platform-server": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@nguniversal/express-engine": "^16.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.15.2",
    "mongodb": "^4.8.0",
    "mongoose": "^6.5.0",
    "ngx-isr": "^0.5.5",
    "rxjs": "~7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.0",
    "@angular/cli": "~16.0.0",
    "@angular/compiler-cli": "^16.0.0",
    "@nguniversal/builders": "^16.0.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.1",
    "prettier": "^2.8.8",
    "typescript": "~5.0.2"
  }
